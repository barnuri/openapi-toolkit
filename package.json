{
    "name": "openapi-definition-to-editor",
    "version": "1.0.81",
    "description": "",
    "main": "./dist/index.js",
    "types": "./dist/index.d.ts",
    "files": [
        "dist"
    ],
    "scripts": {
        "start": "nodemon",
        "format": "prettier --write \"src/**/*.{js,jsx,ts,tsx}\"",
        "preupload": "rimraf dist && tsc",
        "upload": "npm version patch && npm publish",
        "postupload": "git push",
        "generate:test": "tsc && openapi-definition-generate -i ./src/openapiSchemaExample.json -g typescript-axios -o ./test/autoGenerated && tsc --build tsconfig-test.json"
    },
    "bin": {
        "openapi-definition-generate": "./dist/cli-generate.js"
    },
    "author": "Bar Nuri",
    "license": "ISC",
    "homepage": "https://github.com/barnuri/openapi-definition-to-editor",
    "repository": {
        "type": "git",
        "url": "https://github.com/barnuri/openapi-definition-to-editor"
    },
    "dependencies": {
        "rimraf": "^3.0.2",
        "axios": "^0.21.1",
        "jsonpath": "^1.1.0",
        "jsonpath-plus": "^5.0.2",
        "simple-jsonpath": "^1.0.1"
    },
    "devDependencies": {
        "@types/jsonpath": "^0.2.0",
        "@types/node": "^14.14.25",
        "nodemon": "^2.0.7",
        "prettier": "^2.2.1",
        "tslib": "^2.1.0",
        "typescript": "^4.1.3"
    }
}
