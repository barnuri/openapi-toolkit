{
    "name": "openapi-toolkit",
    "version": "2.0.11",
    "description": "generation of API client libraries (SDK generation)",
    "main": "./dist/index.js",
    "types": "./dist/index.d.ts",
    "files": [
        "dist"
    ],
    "scripts": {
        "start": "nodemon",
        "format": "prettier --write \"src/**/*.{js,jsx,ts,tsx}\"",
        "preupload": "rimraf dist && tsc",
        "upload": "npm version patch && npm publish",
        "postupload": "git push",
        "test:typescript": "tsc && openapi-toolkit -i ./src/openapiSchemaExample.json -g typescript-axios -o ./test --modelNamePrefix My --modelNameSuffix .dto.test && tsc --build tsconfig-test.json",
        "test:csharp": "tsc && openapi-toolkit -i ./src/openapiSchemaExample.json -g c# -o ./test-csharp --modelNamePrefix My",
        "test:python": "tsc && openapi-toolkit -i ./src/openapiSchemaExample.json -g python -o ./test-python --modelNamePrefix My --modelNameSuffix .dto.test"
    },
    "bin": {
        "openapi-toolkit": "./dist/cli-generate.js"
    },
    "author": "Bar Nuri",
    "license": "ISC",
    "homepage": "https://github.com/barnuri/openapi-toolkit",
    "repository": {
        "type": "git",
        "url": "https://github.com/barnuri/openapi-toolkit"
    },
    "dependencies": {
        "axios": "^0.21.1",
        "jsonpath-plus": "^6.0.1",
        "rimraf": "^3.0.2",
        "yargs": "^17.0.1"
    },
    "devDependencies": {
        "@types/jsonpath": "^0.2.0",
        "@types/node": "^16.4.7",
        "@types/yargs": "^17.0.2",
        "nodemon": "^2.0.12",
        "prettier": "^2.3.2",
        "tslib": "^2.3.0",
        "typescript": "^4.3.5"
    },
    "publishConfig": {
        "registry": "https://registry.npmjs.org"
    }
}
